<!--logs.wxml-->
<view class="container log-list">
<button catchtap="chooseImageTap">选择封面图片</button>
<!-- forms 表单提交数据  -->
 <view>
  <form bindsubmit="publishCourse" bindreset="formReset">
   <image name="img_url"  class="xd-img-user1"  src="{{logo}}"></image>
  <input placeholder-class="input-holder" type="number" maxlength="11" placeholder="请输入号码" auto-focus></input>
  <radio-group name="gender" bindchange="radioChange">
    <radio value="0" checked />女士
    <radio value="1" />先生
  </radio-group>
 
  <button formType="submit">提交</button>
</form>
 </view>
</view>
